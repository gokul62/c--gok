#include <stdio.h>

void swap(int *x,int *y)  // return type should be void
{
    int temp = *x;
    *x = *y;
    *y = temp;
}

int main()   // standard form
{
    int a, b;
    
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);   // removed \n
    
    printf("Before swapping: a = %d, b = %d\n", a, b);
    
    swap(&a, &b);
    
    printf("After swapping: a = %d, b = %d\n", a, b);
    
    return 0;
}
